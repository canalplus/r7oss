	soc {
		/* FC1 VSAFE */
		pin-controller-sbc {
			/* PIO SBC */
			#address-cells = <2>;
			#size-cells = <1>;
			compatible = "st,sti8416-sbc-pinctrl";
			st,syscfg = <&syscfg_pio_sbc>;
			reg = <0 0x0a50f080 0x4>;
			reg-names = "irqmux";
			interrupts = <GIC_SPI 188 IRQ_TYPE_NONE>;
			interrupts-names = "irqmux";
			ranges = <0 0 0x0 0x0a500000 0x7000>;

			/* SBC PIOs */
			PIO0: PIO@0a500000 {
				gpio-controller;
				#gpio-cells = <2>;
				interrupt-controller;
				#interrupt-cells = <2>;
				reg = <0x0 0x0 0x100>;
				st,bank-name = "PIO0";
			};

			PIO1: PIO@0a501000 {
				gpio-controller;
				#gpio-cells = <2>;
				interrupt-controller;
				#interrupt-cells = <2>;
				reg = <0x0 0x1000 0x100>;
				st,bank-name = "PIO1";
			};

			PIO2: PIO@0a502000 {
				gpio-controller;
				#gpio-cells = <2>;
				interrupt-controller;
				#interrupt-cells = <2>;
