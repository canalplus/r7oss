		gpio8 = &PIO11;
		gpio9 = &PIO12;
		gpio10 = &PIO13;
		gpio11 = &PIO14;
		gpio12 = &PIO15;
		gpio13 = &PIO16;
		gpio14 = &PIO17;
		gpio15 = &PIO18;
		gpio16 = &PIO19;
		gpio17 = &PIO20;
