			PIO5: PIO@0a505000 {
				gpio-controller;
				#gpio-cells = <2>;
				interrupt-controller;
				#interrupt-cells = <2>;
				reg = <0x0 0x5000 0x100>;
				st,bank-name = "PIO5";
